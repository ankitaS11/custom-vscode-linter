{
  "name": "custom-vscode-linter",
  "displayName": "custom-vscode-linter",
  "description": "Custom VSCode Linter!",
  "version": "0.7.3",
  "publisher": "hchiam",
  "repository": {
    "type": "git",
    "url": "https://github.com/hchiam/custom-vscode-linter"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [{
      "command": "extension.custom-vscode-linter",
      "title": "Custom VSCode Linter"
    }]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "npm run compile"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "@types/vscode": "^1.66.0",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  }
}